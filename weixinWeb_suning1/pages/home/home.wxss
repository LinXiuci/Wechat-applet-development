/* 导航顶部栏设置 */
.swiper-tab{
    white-space: nowrap; /* 实现水平滚动的重要属性 */
    display: flex;
    width: 100%;
    height: 80rpx;  /* 去掉高度可以全部显示 */
    text-align: center;
    line-height: 75rpx;
   
}
.swiper-tab-list{
    font-size: 30rpx;
    display: inline-block;
    width: 20%;
    color: #ffffff;
}
.on{
    color: #F4D03F;
    border-bottom: 5rpx solid #da7c0c;
}
.swiper-box{
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
}
.swiper-box view{
    text-align: center;
}


/* <!-- 轮播图片 --> */
.swiper-image-area{
    width: 100%;
    height: 245rpx;
    background: url('https://image3.suning.cn/uimg/cms/img/161293982654037818.jpg');
    border-radius: 0 0 20% 20%;
}
.swiper-image-area view{
    width: 95%;
    height: 250rpx;
    margin: auto;
    border-radius: 20rpx;
}
.swiper-image-area image{
    width: 100%;
    height: 245rpx;
    border-radius: 20rpx;
}
.swiper-image-area swiper{
    width: 100%;
    height: 250rpx;
}


